{
    "require": {
        "fzaninotto/faker": "^1.9",
        "codeigniter/framework": "3.1.11",
        "mpdf/mpdf" : "^8.0.10",
        "filp/whoops": "^2.7",
        "mk-j/php_xlsxwriter": "^0.38.0",
        "mpdf/qrcode": "^1.1",
        "phpoffice/phpword": "^0.18.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "symfony/var-dumper": "^5.0"
    },
    "scripts": {
        "format": [
            "application/vendor/bin/php-cs-fixer fix"
